{
    "homepage": "https://railmapgen.github.io/rmg-translate",
    "name": "@railmapgen/rmg-translate",
    "repository": "github:railmapgen/rmg-translate",
    "bugs": {
        "url": "https://github.com/railmapgen/rmg-translate/issues"
    },
    "license": "GPL-3.0-only",
    "devDependencies": {
        "@types/node": "^16.11.47",
        "@typescript-eslint/eslint-plugin": "^5.33.1",
        "@typescript-eslint/parser": "^5.33.1",
        "concurrently": "^7.3.0",
        "eslint": "8.22.0",
        "eslint-plugin-prettier": "^4.2.1",
        "fork-ts-checker-webpack-plugin": "^7.2.13",
        "html-webpack-plugin": "^5.5.0",
        "i18next": "^21.9.1",
        "interpolate-html-plugin": "^4.0.0",
        "prettier": "^2.7.1",
        "ts-loader": "^9.3.1",
        "typescript": "^4.7.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.10.1"
    },
    "peerDependencies": {
        "i18next": ">=21"
    },
    "scripts": {
        "start": "webpack serve",
        "build:package": "tsc -p ./tsconfig.prd.json",
        "build:ui": "webpack --mode=production",
        "build": "npm run build:package && npm run build:ui",
        "test": "echo \"No tests found\"",
        "lint": "eslint -c ./.eslintrc.dev.json ./src ./ui",
        "lint:fix": "eslint ./src ./ui --fix"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "version": "0.3.1",
    "jest": {
        "transformIgnorePatterns": [
            "/node_modules/(?!(@railmapgen/.*))"
        ],
        "moduleNameMapper": {
            "\\.(css|less)$": "identity-obj-proxy"
        }
    },
    "main": "./dist/index.js",
    "exports": {
        ".": "./dist/index.js",
        "./**/*": "./dist/**/*"
    },
    "files": [
        "dist"
    ]
}
